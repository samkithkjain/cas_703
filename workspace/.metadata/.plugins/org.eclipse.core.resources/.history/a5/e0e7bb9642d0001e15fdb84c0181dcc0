	public static boolean isSemanticLeafVisualID(int visualID) {
		switch (visualID) {
						case carnival.diagram.edit.parts.CarnivalEditPart.VISUAL_ID:
				return false;
									case carnival.diagram.edit.parts.VisitorEditPart.VISUAL_ID:case carnival.diagram.edit.parts.VolunteerEditPart.VISUAL_ID:case carnival.diagram.edit.parts.BoothEditPart.VISUAL_ID:case carnival.diagram.edit.parts.EventEditPart.VISUAL_ID:
				return true;
									default:
				break;
		}
		return false;
	}

